    "bluebird": "^3.4.0",
    "eslint": "^2.10.2",
    "ramda": "^0.21.0",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "rollup": "^0.26.3",
    "rollup-plugin-babel": "^2.4.0",
